<?xml version="1.0" encoding="UTF-8" standalone="no"?>
<gbSubmisisonGenerator>
	<assemblyGapSpecifier>
		<lengthsKnown>false</lengthsKnown>
		<minRunLengthForGap>50</minRunLengthForGap>
		<linkageEvidence>align-genus</linkageEvidence>
	</assemblyGapSpecifier>
	<structuredCommentProvider>
		<sequencingTechnology>Oxford Nanopore MinION</sequencingTechnology>
		<assemblyMethod>Guppy</assemblyMethod>
		<assemblyMethod>minimap2</assemblyMethod>
		<assemblyMethod>medaka</assemblyMethod>
	</structuredCommentProvider>
	<sourceInfoProviders>
		<staticSourceInfoProvider>
			<sourceModifier>organism</sourceModifier>
			<value>Rabies lyssavirus</value>
		</staticSourceInfoProvider>
		<staticSourceInfoProvider>
			<sourceModifier>moltype</sourceModifier>
			<value>genomic</value>
		</staticSourceInfoProvider>
		<staticSourceInfoProvider>
			<sourceModifier>molecule</sourceModifier>
			<value>rna</value>
		</staticSourceInfoProvider>
		<freemarkerSourceInfoProvider>
			<sourceModifier>genotype</sourceModifier>
			<template><![CDATA[<#list alignmentMemberships as memb><#if memb.alignment.refSequence??>${memb.alignment.displayName}</#if></#list>]]></template>
		</freemarkerSourceInfoProvider>
		<freemarkerSourceInfoProvider>
			<sourceModifier>host</sourceModifier>
			<template>${host!""}</template>
		</freemarkerSourceInfoProvider>
		<freemarkerSourceInfoProvider>
			<sourceModifier>isolate</sourceModifier>
			<template>${isolate!""}</template>
		</freemarkerSourceInfoProvider>
		<freemarkerSourceInfoProvider>
			<sourceModifier>isolation_source</sourceModifier>
			<template>${iso_source!""}</template>
		</freemarkerSourceInfoProvider>
		<freemarkerSourceInfoProvider>
			<sourceModifier>collection-date</sourceModifier>
			<template>${(collection_year?c)!""}</template>
		</freemarkerSourceInfoProvider>
		<freemarkerSourceInfoProvider>
			<sourceModifier>note</sourceModifier>
			<template>${rabv_submission_gb_notes!""}</template>
		</freemarkerSourceInfoProvider>
		<freemarkerSourceInfoProvider>
			<sourceModifier>country</sourceModifier>
			<template>${gb_country!""}</template>
		</freemarkerSourceInfoProvider>
	</sourceInfoProviders>
